<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="co.edu.uniquindio.sistemagestionhospital.viewController.NotificacionesViewController"
            prefHeight="400.0" prefWidth="600.0">

    <VBox alignment="CENTER" spacing="15.0" layoutX="100.0" layoutY="50.0" prefWidth="400.0">
        <Label text="Notificaciones Recibidas" style="-fx-font-size: 18px; -fx-font-weight: bold;" />

        <ListView fx:id="listaNotificaciones" prefHeight="250.0" prefWidth="400.0" />

        <Button text="Cerrar" onAction="#cerrarVentana" prefWidth="100.0" />
    </VBox>

</AnchorPane>
